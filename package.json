{
  "name": "auth_service",
  "repository": "git@gitlab.karlson.dev:individual/pet_gonote/authservice.git",
  "author": "vulgarnykarlson <vulgarnykarlson@gmail.com>",
  "version": "0.1.0",
  "scripts": {
    "build": "yarn clean && tsc --project tsconfig.build.json",
    "prisma:generate": "DATABASE_URL=$(node -p \"require('./dbConfig')\") prisma generate",
    "prisma:push": "DATABASE_URL=$(node -p \"require('./dbConfig')\") prisma db push",
    "prisma:migrate": "DATABASE_URL=$(node -p \"require('./dbConfig')\") prisma migrate dev",
    "clean": "rimraf dist",
    "lint": "eslint \"./src/**\" --fix",
    "start:dev": "node -r ts-node/register -r  tsconfig-paths/register src/main.ts",
    "start:prod": "NODE_PATH=dist TS_NODE_PROJECT='tsconfig.run.json' node -r tsconfig-paths/register dist/main.js"
  },
  "dependencies": {
    "@nestjs/common": "^10.1.3",
    "@nestjs/core": "^10.1.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.1.3",
    "@prisma/client": "^5.1.1",
    "argon2": "^0.31.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cli-color": "^2.0.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cli-color": "^2.0.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@types/passport-jwt": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sonarjs": "^0.20.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prisma": "^5.1.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "packageManager": "yarn@3.2.3",
  "engines": {
    "node": ">=18.7.0",
    "yarn": ">=3.2.3"
  }
}
